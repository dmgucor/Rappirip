package extras;

import java.awt.Color;

public enum Colores {
	LM_FONDO("#EEEEEE"), 
	LM_LABEL("#A3A3A3"), 
	DM_LABEL("#C2C2C2"), 
	DM_FONDO("#000000"), 
	LM_MAIN("#007991"),
	DM_MAIN("#1F2947"),
	LM_TEXTFIELD("#FFFFFF"),
	DM_TEXTFIELD("#5C5C5C"),
	LM_TEXTFIELD_BORDER("#005566"),
	LM_TEXTFIELD_SHADE("#00778F"),
	DM_TEXTFIELD_BORDER("#263154"),
	DM_TEXTFIELD_SHADE("#263154"),;

	private String hex;

	private Colores(String hex) {
		this.hex = hex;
	}

	public Color getColor() {
		return Color.decode(hex);
	}

}
