package extras;

import java.awt.Color;

public enum Colores {
	LM_FONDO("#EEEEEE"), 
	LM_LABEL("#A3A3A3"), 
	DM_LABEL("#DA4167"), 
	DM_FONDO("#000000"), 
	LM_MAIN("#007991"),
	DM_MAIN("#1F2947"),
	LM_TEXTFIELD("#FFFFFF"),
	DM_TEXTFIELD("#A3A3A3"),
	LM_TEXTFIELD_BORDER("##005566"),
	LM_TEXTFIELD_SHADE("#00778F"),
	DM_TEXTFIELD_BORDER("#334171"),
	DM_TEXTFIELD_SHADE("#46599B"),;

	private String hex;

	private Colores(String hex) {
		this.hex = hex;
	}

	public Color getColor() {
		return Color.decode(hex);
	}

}
