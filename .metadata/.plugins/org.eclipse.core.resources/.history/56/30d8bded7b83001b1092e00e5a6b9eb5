package extras;

import java.awt.Color;

public enum Colores {
	LM_FONDO("#EEEEEE"), 
	LM_LABEL("#F08080"), 
	DM_LABEL("#FBD1A2"), 
	DM_FONDO("#000000"), 
	LM_MAIN("#007991"),
	DM_MAIN("#1F2947"),
	LM_TEXTFIELD("#FFFFFF"),
	DM_TEXTFIELD("#525252"),;

	private String hex;

	private Colores(String hex) {
		this.hex = hex;
	}

	public Color getColor() {
		return Color.decode(hex);
	}

}
