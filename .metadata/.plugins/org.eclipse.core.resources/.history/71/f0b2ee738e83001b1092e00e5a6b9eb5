package extras;

import java.awt.Color;

public enum Colores {
	LM_FONDO("#EEEEEE"), 
	LM_LABEL("#00333D"), 
	DM_LABEL("#C2C2C2"), 
	DM_FONDO("#000000"), 
	LM_MAIN("#a3ddcb"),
	DM_MAIN("#1F2947"),
	LM_TEXTFIELD("#FFFFFF"),
	DM_TEXTFIELD("#5C5C5C"),
	LM_TEXTFIELD_BORDER("#a3ddcb"),
	LM_TEXTFIELD_SHADE("#5BC2A2"),
	DM_TEXTFIELD_BORDER("#263154"),
	DM_TEXTFIELD_SHADE("#263154"),
	LM_HYPERLINK("#9D00E0"),
	LM_HYPERLINK_ENTERED("#470066"),
	DM_HYPERLINK("#D6B11F"),
	DM_HYPERLINK_ENTERED("#E8CD5E"),;

	private String hex;

	private Colores(String hex) {
		this.hex = hex;
	}

	public Color getColor() {
		return Color.decode(hex);
	}

}
