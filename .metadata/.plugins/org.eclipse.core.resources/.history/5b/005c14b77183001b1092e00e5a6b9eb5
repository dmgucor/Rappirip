package extras;

import java.awt.Color;

public enum Colores {
	LM_FONDO("#EEEEEE"), LM_LABEL("#000000"), DM_LABEL("#FFFFFF"), DM_FONDO("#000000"), MAIN("#E8871E") ;

	private String hex;

	private Colores(String hex) {
		this.hex = hex;
	}

	public Color getColor() {
		return Color.decode(hex);
	}

}
